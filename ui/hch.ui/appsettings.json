{

  //部署端口配置
  "server.urls": "http://*:9901",

  "FileServerConfig": {
    "Configs": [
      {
        "FType": "IMAGE", //文件类型
        "FileServerLocal": "/upload/image/", //文件下载保存的本地路径
        "AllowedExt": [ "jpg", "png", "jpeg", "gif", "bmp" ], //允许上传的扩展名
        "Size": 1024, //允许上传的最大值kb
        "Count": 6 //允许上传的张数
      }
    ]
  },


  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
